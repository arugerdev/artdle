{
  "functions": {
    "api/draw.js": {
      "runtime": "nodejs12.x"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    },
    {
      "source": "/draw/(.*)",
      "destination": "/draw.js"
    }
  ],
  "routes": [
    {
      "src": "/api/draw/([^/]+)",
      "dest": "/api/draw.js"
    },
    {
      "src": "/api/draw/(.*)",
      "dest": "/api/draw.js"
    },
    {
      "src": "/draw/([^/]+)",
      "dest": "/api/draw.js"
    },
    {
      "src": "/draw/(.*)",
      "dest": "/api/draw.js"
    }
  ]
}
